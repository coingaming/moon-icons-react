{
  "name": "@heathmont/moon-icons-tw",
  "sideEffects": false,
  "version": "9.29.3",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "typings": "lib/index.d.ts",
  "repository": "https://github.com/coingaming/moon-icons-react",
  "author": "Yolo Group",
  "license": "GPL-3.0-only",
  "scripts": {
    "prepublishOnly": "pnpm run publish:pre:move:lib && pnpm run publish:pre:update:package",
    "postpublish": "pnpm run publish:post:unmove:lib && pnpm run publish:post:revert:package",
    "publish:pre:move:lib": "node ./.config/scripts/publish.js --input ./lib/ --output ./ --mode prepublish",
    "publish:pre:update:package": "node ./.config/scripts/updatePackageJson.js --input ./lib/ --package ./package.json --mode prepublish",
    "publish:post:unmove:lib": "node ./.config/scripts/publish.js --input ./lib/ --output ./ --mode postpublish",
    "publish:post:revert:package": "node ./.config/scripts/updatePackageJson.js --input ./lib/ --package ./package.json --mode postpublish",
    "figma": "node scripts/import/index.js --token $(grep FIGMA_TOKEN .env | cut -d '=' -f2) --fileId $(grep FIGMA_FILE_ID .env | cut -d '=' -f2) --page $npm_config_page  --dir ./svg",
    "figma:page": "npm run figma --page=Dev-MDSMainIconSet",
    "build:icons": "svgr --ext tsx --config-file ./.config/svgr/svgr.config.js -d src/icons svg",
    "build:index": "barrelsby --config ./.config/barrelsby/barrelsby.json --directory src --structure flat --exportDefault",
    "build:code": "pnpm run build:js:es && pnpm run build:js:commonjs",
    "build:js:commonjs": "tsc --project tsconfig.cjs.json",
    "build:js:es": "tsc --project tsconfig.esm.json",
    "build": "pnpm run build:icons && pnpm run build:code && pnpm run build:index",
    "lint": "eslint ./.config/eslint --ext .js,.jsx,.ts,.tsx",
    "chokidar": "chokidar \"**/*.tsx\" \"**/*.ts\" -i \"lib/**/*.d.ts\" -c \"pnpm run build:code\" "
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "4.1.0",
    "@types/node": "13.13.52",
    "@types/ramda": "0.26.44",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/rimraf": "2.0.5",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "argparse": "2.0.1",
    "axios": "0.21.4",
    "barrelsby": "2.2.0",
    "commander": "4.0.1",
    "eslint": "7.18.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.11.0",
    "figma-js": "1.14.0",
    "figma-transformer": "2.1.0",
    "fs": "0.0.1-security",
    "prettier": "2.3.0",
    "ramda": "0.27.1",
    "rimraf": "3.0.2",
    "ts-node-dev": "1.1.8",
    "typescript": "4.1.4"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0"
  }
}
